programa {
  funcao inicio() {
      inteiro tamanhoSequencia, contador = 1

      escreva("Vamos escrever uma sequência de números!\nDigite o número da sequência:\n")
      leia(tamanhoSequencia)
      escreva("Tu desejas a sequência do número: ", tamanhoSequencia, ".\n")

      enquanto(contador  <= tamanhoSequencia){
        escreva(contador, " ")
        contador = contador + 1
      }


    
   

    
  }
}
