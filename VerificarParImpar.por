programa {
  funcao inicio() {
    //FAZER UM ALGORITMO QUE RECEBA DOIS VALORES. INFORMAR SE É PAR OU ÍMPAR;

    inteiro valor1

    escreva("Digite um valor: \n")
    leia(valor1)

    se(valor1 % 2 == 0){
        escreva("O resultado é par.")
    }
    senao{
        escreva("O resultado é ímpar.")
    }
  }
}
